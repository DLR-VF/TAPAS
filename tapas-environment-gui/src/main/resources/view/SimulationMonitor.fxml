<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="809.0" prefWidth="1065.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextField />
                  <Button mnemonicParsing="false" text="Button" />
               </children>
            </HBox>
            <TableView fx:id="serverTable" prefHeight="200.0" prefWidth="1045.0">
              <columns>
                <TableColumn fx:id="serverName" prefWidth="75.0" text="name" />
                <TableColumn fx:id="serverIpAddress" prefWidth="75.0" text="IP-address" />
                  <TableColumn fx:id="serverCpuUsage" prefWidth="75.0" text="CPU-usage" />
                  <TableColumn fx:id="serverCoreCount" prefWidth="75.0" text="core count" />
                  <TableColumn fx:id="serverOnline" prefWidth="75.0" text="online" />
              </columns>
            </TableView>
            <TableView fx:id="simulationTable" layoutX="10.0" layoutY="110.0" prefHeight="200.0" prefWidth="1045.0">
               <columns>
                  <TableColumn fx:id="simId" prefWidth="75.0" text="id" />
                  <TableColumn fx:id="simName" prefWidth="75.0" text="name" />
                  <TableColumn fx:id="simDescription" prefWidth="75.0" text="description" />
                  <TableColumn fx:id="simReady" prefWidth="75.0" text="ready" />
                  <TableColumn fx:id="simStarted" prefWidth="75.0" text="started" />
                  <TableColumn fx:id="simFinished" prefWidth="75.0" text="finished" />
                  <TableColumn fx:id="simProgress" prefWidth="75.0" text="progress" />
                  <TableColumn fx:id="simCount" prefWidth="75.0" text="count" />
                  <TableColumn fx:id="simStart" prefWidth="75.0" text="start date" />
                  <TableColumn fx:id="simEnd" prefWidth="75.0" text="end date" />
                  <TableColumn fx:id="simElapsedTime" prefWidth="75.0" text="elapsed time" />
               </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
